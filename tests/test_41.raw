ОПРЕДЕЛИТЬ ПРОЦЕДУРУ главная() (
    ЗАДАТЬ тестовый_сценарий = ТестовыйСценарий();

    ЗАДАТЬ задача = В ФОНЕ фон();

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(главная);
    )
    ОБРАБОТЧИК ОшибкаЗначения КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(задача);
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(массив());
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(таблица());
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число("");
    )
    ОБРАБОТЧИК ОшибкаЗначения КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число("2", "ЛИШНИЙ АРГУМЕНТ");
    )
    ОБРАБОТЧИК ОшибкаАргумента КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(1);
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(1.0);
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число("1");
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число("1.0");
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(ИСТИНА);
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    КОНТЕКСТ (
        НАПЕЧАТАТЬ в_число(ЛОЖЬ);
    )
    ОБРАБОТЧИК ОшибкаТипа КАК о (
        НАПЕЧАТАТЬ о;
    )

    тестовый_сценарий:простой_тест(в_число("2") + в_число("2") * в_число("2") РАВНО 6.0);
    тестовый_сценарий:простой_тест(в_число(в_число("2") + в_число("2") * в_число("2")) * 100 РАВНО 600.0);
    тестовый_сценарий:простой_тест(в_число(в_строку(в_число(в_число("2") + в_число(в_строку(в_число("2"))) * в_число("2")))) * 1000 РАВНО 6000.0);

    ЖДАТЬ задача;
)

ВЫПОЛНИТЬ (
    главная();
)
