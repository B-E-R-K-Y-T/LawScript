ВКЛЮЧИТЬ стандартная_библиотека.*


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тестовая_1(арг=ПУСТОТА) (
    НАПЕЧАТАТЬ арг;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тестовая_2(арг=1) (
    НАПЕЧАТАТЬ арг;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тестовая_3(арг=2 + 2 * 2) (
    НАПЕЧАТАТЬ арг;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тестовая_4(арг=массив(массив(1, 2, 3))) (
    НАПЕЧАТАТЬ арг;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тестовая_5(арг=тестовая_4) (
    НАПЕЧАТАТЬ арг;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тестовая_6(арг1=1, арг2=2) (
    НАПЕЧАТАТЬ арг;
)

ОПРЕДЕЛИТЬ КЛАСС Тестовый_1 (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка) (тест=временная_метка()) (
        ссылка:тест = тест;
        НАПЕЧАТАТЬ ссылка:тест;
    )

    ОПРЕДЕЛИТЬ МЕТОД (ссылка) запуск () (
        НАПЕЧАТАТЬ ссылка:тест;
    )
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ главная() (
    тестовая_1();
    тестовая_2();
    тестовая_3();
    тестовая_4();
    тестовая_5();

    ЗАДАТЬ тестовый_1 = Тестовый_1();
    тестовый_1:запуск();
    ЗАДАТЬ тестовый_2 = Тестовый_1(115);
    тестовый_2:запуск();
    ЗАДАТЬ тестовый_3 = Тестовый_1(тестовый_2:тест * 2);
    тестовый_3:запуск();
)

ВЫПОЛНИТЬ (
    главная();
)
