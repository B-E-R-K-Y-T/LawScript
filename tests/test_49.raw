ВКЛЮЧИТЬ стандартная_библиотека.*


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ разбор(мас) (
    ЗАДАТЬ элемент;

    ЦИКЛ индекс ОТ 0 ДО длина_массива(мас) - 1 (
        элемент = достать_из_массива(мас, индекс);

        ЕСЛИ получить_тип(элемент) РАВНО "Массив" ТО (
            разбор(элемент);
            ПРОПУСТИТЬ;
        )

        НАПЕЧАТАТЬ элемент;
    )
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ запуск_программы() (
    ЗАДАТЬ мас = массив(1);
    добавить_в_массив(мас, мас);

    НАПЕЧАТАТЬ достать_из_массива(достать_из_массива(мас, 1), 1);

    ЗАДАТЬ т = таблица();

    добавить_в_таблицу(т, "1", т);

    НАПЕЧАТАТЬ т;

    добавить_в_массив(мас, т);

    НАПЕЧАТАТЬ мас;
    НАПЕЧАТАТЬ получить_тип(мас);

    разбор(массив(1, 2, 3, массив(6, 6, 6), массив(), 4, 5, 6));

    1 + 1 = 1 + 1;
    КОНТЕКСТ (
        разбор(мас);
    )
    ОБРАБОТЧИК БазоваяОшибка КАК _ (
        НАПЕЧАТАТЬ "Обнаружена ошибка при попытке достать элемент из массива!";
        ! TODO:  Не падает в ошибку, хотя должен был
        ПРЕРВАТЬ;
    )



)

ВЫПОЛНИТЬ (
    запуск_программы();
)
