ВКЛЮЧИТЬ стандартная_библиотека.*

!НАСЛЕДОВАТЬ КЛАСС имя_класса_2 ОТ имя_класса (
!)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ ф(арг)(

    !метод(1);
    класс:метод(2);
    !1+ класс:метод:a(2);
)

ОПРЕДЕЛИТЬ КЛАСС имя_класса3 (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка)()(
       ссылка:а = 555555;
    )
    ОПРЕДЕЛИТЬ МЕТОД (ссылка) метод(арг)(
       НАПЕЧАТАТЬ "Привет из ВТОРОГО в этом ЯП метода)" * арг;
    )
)

ОПРЕДЕЛИТЬ КЛАСС имя_класса2 (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка)()(
      ссылка:а = 114;
       ссылка:имя_класса2_a = имя_класса3();
    )
)

ОПРЕДЕЛИТЬ КЛАСС имя_класса (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка)(а, б)(
      ссылка:а = а;
      ссылка:иной_класс = имя_класса2();
       ! НАПЕЧАТАТЬ ссылка:а;
    )
    ОПРЕДЕЛИТЬ МЕТОД (ссылка) метод(арг)(
       НАПЕЧАТАТЬ "Привет из первого в этом ЯП метода)" * арг;
    )
    ОПРЕДЕЛИТЬ МЕТОД (ссылка) метод1()(
        ВЕРНУТЬ 1;
    )
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ процедура()(
    !3 + метод(1);

    ЗАДАТЬ класс = имя_класса("Привет ", "первый класс!");
    ЗАДАТЬ класс2 = имя_класса("Пока ", "первый класс!");

    НАПЕЧАТАТЬ "Каст в строку: " + в_строку(класс:иной_класс:имя_класса2_a:а ^ 2);
    НАПЕЧАТАТЬ класс:а;
    НАПЕЧАТАТЬ класс2:а;
    класс:метод(2);
    НАПЕЧАТАТЬ (класс:метод1() + 1) ^ 2;
    ЗАДАТЬ ч = класс:метод;
    ЗАДАТЬ ч1 = класс:метод1;
    НАПЕЧАТАТЬ ч;
    НАПЕЧАТАТЬ ч1;

    НАПЕЧАТАТЬ класс:иной_класс:а;
    НАПЕЧАТАТЬ форматировать_строку("класс:иной_класс:имя_класса2_a:а={}", класс:иной_класс:имя_класса2_a:а);
    НАПЕЧАТАТЬ класс:иной_класс:имя_класса2_a:а РАВНО 1;
    НАПЕЧАТАТЬ класс:иной_класс:имя_класса2_a:а ^ 2;

    НАПЕЧАТАТЬ "Каст в строку класс:иной_класс:имя_класса2_a:а ^ 2: " + в_строку(класс:иной_класс:имя_класса2_a:а ^ 2);
    НАПЕЧАТАТЬ "Каст в строку (класс:метод1() + 1) ^ 2: " + в_строку((класс:метод1() + 1) ^ 2);

    ЗАДАТЬ класс12 = имя_класса("Привет1212 ", "первый класс!");
    НАПЕЧАТАТЬ класс12:а;

    класс:тест = 1;
    НАПЕЧАТАТЬ класс:а;
    НАПЕЧАТАТЬ класс:тест;
    НАПЕЧАТАТЬ класс12:тест;

    НАПЕЧАТАТЬ класс:тест + 1;
    НАПЕЧАТАТЬ класс:тест;
    класс:тест = класс:тест + 1;
    НАПЕЧАТАТЬ класс:тест + 2 * 2;
    НАПЕЧАТАТЬ (класс:тест + 2) * 2;



)

ВЫПОЛНИТЬ (
    процедура();
)
