ВКЛЮЧИТЬ стандартная_библиотека.структуры.примитивные_структуры
ВКЛЮЧИТЬ стандартная_библиотека.утилиты


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ отобразить(отображаемая_функция, мас, старт=0)(
    ЗАДАТЬ длина = длина_массива(мас);

    ЕСЛИ длина РАВНО 0 ТО (
        ВЕРНУТЬ 0;
    )

    ЗАДАТЬ результат = старт;

    ЦИКЛ счет ОТ 0 ДО длина - 1 (
        результат = отображаемая_функция(результат, достать_из_массива(мас, счет));
    )

    ВЕРНУТЬ результат;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ сумма(а, б) (
    ВЕРНУТЬ а + б;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ умножение(а, б) (
    ВЕРНУТЬ а * б;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ степень(а, б) (
    ВЕРНУТЬ а ^ б;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ разность(а, б) (
    ВЕРНУТЬ а - б;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ повторение(а) (
    ВЕРНУТЬ а * 2;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ вход_в_программу()(
    НАПЕЧАТАТЬ отобразить(сумма, массив(3, 3, 3));
    НАПЕЧАТАТЬ отобразить(сумма, массив());

    НАПЕЧАТАТЬ отобразить(умножение, массив(3, 3, 3), 1);
    НАПЕЧАТАТЬ отобразить(умножение, массив(), 1);

    НАПЕЧАТАТЬ отобразить(степень, массив(3, 3, 3), 2);
    НАПЕЧАТАТЬ отобразить(степень, массив(), 2);

    НАПЕЧАТАТЬ отобразить(разность, массив(3, 3, 3));
    НАПЕЧАТАТЬ отобразить(разность, массив());

    ЗАДАТЬ ключи = массив("умножение");
    ЗАДАТЬ функции = массив(умножение);

    ЗАДАТЬ варианты = таблица(ключи, функции);

    НАПЕЧАТАТЬ варианты;

    НАПЕЧАТАТЬ отобразить(извлечь_из_таблицы(варианты, "умножение"), массив(3, 3, 3), 1);
    НАПЕЧАТАТЬ отобразить(извлечь_из_таблицы(варианты, "умножение"), массив(), 1);

    НАПЕЧАТАТЬ отобразить_на_массив(повторение, массив("1", "2", "3"));
    НАПЕЧАТАТЬ отобразить_на_массив(повторение, массив());
)

ВЫПОЛНИТЬ (
    вход_в_программу();
)
