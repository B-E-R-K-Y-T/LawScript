ВКЛЮЧИТЬ стандартная_библиотека.строки


ОПРЕДЕЛИТЬ КЛАСС Ошибкис НАСЛЕДОВАТЬ ОТ БазоваяОшибка (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка) () (
        ссылка:информация = "АХАХАХА";
    )
)

ОПРЕДЕЛИТЬ КЛАСС Ошибкис2 (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка) () (
        ссылка:информация = "АХАХАХА";
    )
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ вход() (
    КОНТЕКСТ (
        ОШИБКА ОшибкаДелениеНаНоль;
    )
    ОБРАБОТЧИК ОшибкаДелениеНаНоль КАК ОШ (
        НАПЕЧАТАТЬ "ОшибкаДелениеНаНоль...";
      !НАПЕЧАТАТЬ ОШ:информация;
       !НАПЕЧАТАТЬ ОШ:__ошибка__;
   )
    ОБРАБОТЧИК Ошибкис КАК ОШ (
        НАПЕЧАТАТЬ "Ошибкис...";
       НАПЕЧАТАТЬ ОШ:информация;
       НАПЕЧАТАТЬ ОШ:__ошибка__;
   )
    ОБРАБОТЧИК БазоваяОшибка КАК ОШ (
        НАПЕЧАТАТЬ "БазоваяОшибка...";
     НАПЕЧАТАТЬ ОШ:информация;
       НАПЕЧАТАТЬ ОШ:__ошибка__;
   )
)

ВЫПОЛНИТЬ (
	вход();
)
