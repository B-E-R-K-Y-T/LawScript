ВКЛЮЧИТЬ стандартная_библиотека.структуры.примитивные_структуры


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тест()(
    ЗАДАТЬ счетчик = 0;

    ЕСЛИ ИСТИНА ТО (
        НАПЕЧАТАТЬ "если";
        ЕСЛИ ИСТИНА ТО (
            НАПЕЧАТАТЬ "если2";
            НАПЕЧАТАТЬ "если2";
            НАПЕЧАТАТЬ "если2";
            ВЕРНУТЬ ПУСТОТА;
        )
        НАПЕЧАТАТЬ "конец если";
    )

    ЦИКЛ ОТ 1 ДО 2 (
        счетчик = счетчик + 1;
        НАПЕЧАТАТЬ счетчик;
    )

    НАПЕЧАТАТЬ 1;
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ вход_в_программу()(
    НАПЕЧАТАТЬ тест();
)

ВЫПОЛНИТЬ (
    вход_в_программу();
)
