ОПРЕДЕЛИТЬ ПРОЦЕДУРУ фон(номер) (
    ЗАДАТЬ результат = 0;
    ЗАДАТЬ старт = временная_метка();

    ЦИКЛ счет ОТ 1 ДО 1000 (
        результат = результат + (2 + 2 * 2 - счет);
        !НАПЕЧАТАТЬ номер;
    )

    НАПЕЧАТАТЬ форматировать_строку("ЗАДАЧА {} ЗАВЕРШЕНА ЗА ВРЕМЯ: {}", номер, временная_метка() - старт);

    ВЕРНУТЬ результат;
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ главная() (
    ЗАДАТЬ задачи = Список();
    ЗАДАТЬ старт = временная_метка();

    ЦИКЛ номер ОТ 1 ДО 10000 (
        задачи:добавить(В ФОНЕ пустой_фон(номер));
    )

    ждать_всех(задачи:в_массив());
    НАПЕЧАТАТЬ временная_метка() - старт;
)

ВЫПОЛНИТЬ (
    главная();
)
