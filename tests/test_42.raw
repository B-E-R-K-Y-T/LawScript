ВКЛЮЧИТЬ стандартная_библиотека.*


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тест(тест_арг, а=ПУСТОТА, б=ПУСТОТА) (

    ЕСЛИ а РАВНО ПУСТОТА ТО (
        а = форматировать_строку("АРГУМЕНТ номер 1 РАВНО {}", а);
    )

    НАПЕЧАТАТЬ б РАВНО ПУСТОТА;

    ЕСЛИ б РАВНО ПУСТОТА ТО (
        НАПЕЧАТАТЬ "ТЕСТ ";
        б = форматировать_строку("АРГУМЕНТ номер 2 РАВНО {}", б);
    )


    НАПЕЧАТАТЬ а;
    НАПЕЧАТАТЬ б;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ главная() (
    тест(1);
)

ВЫПОЛНИТЬ (
    главная();
)
