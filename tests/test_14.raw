ВКЛЮЧИТЬ стандартная_библиотека.*


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ главная() (
    ЗАДАТЬ старт = временная_метка();
    ЗАДАТЬ результат = 0;

    ЦИКЛ счет ОТ 1 ДО 10000 (
        результат = результат + счет * 2;
    )

    тест();


    НАПЕЧАТАТЬ "конец = " + в_строку(временная_метка()-старт);
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ тест() (
    ЗАДАТЬ а = 2;
    а + 1;
    ЗАДАТЬ к = клс(2);
    НАПЕЧАТАТЬ к:а;
)


ОПРЕДЕЛИТЬ КЛАСС клс (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка) (й) (
        ссылка:а = й;
        НАПЕЧАТАТЬ ссылка:а;
    )
)


ВЫПОЛНИТЬ (
    главная();
)

