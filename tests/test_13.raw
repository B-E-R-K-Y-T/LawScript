ВКЛЮЧИТЬ стандартная_библиотека.рандом
ВКЛЮЧИТЬ стандартная_библиотека.структуры.сложные_структуры


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ проверка_очереди() (
    ЗАДАТЬ тест = Очередь(4);

    НАПЕЧАТАТЬ тест:положить(1);
    НАПЕЧАТАТЬ тест:положить(2);
    НАПЕЧАТАТЬ тест:положить(2);
    НАПЕЧАТАТЬ тест:положить(2);

    НАПЕЧАТАТЬ тест:взять();
    НАПЕЧАТАТЬ тест:взять();
    НАПЕЧАТАТЬ тест:взять();
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ проверка_списков() (
    ЗАДАТЬ с = Список(массив(1, 2, 3));
    !ЗАДАТЬ с = Список();

    с:добавить(1);

    НАПЕЧАТАТЬ с:в_массив();
    НАПЕЧАТАТЬ с:сортировать();

    с:удалить(-1);
    НАПЕЧАТАТЬ с:в_массив();
    НАПЕЧАТАТЬ с:достать();
    НАПЕЧАТАТЬ с:достать(-1);
   ! НАПЕЧАТАТЬ с:достать(10);
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ проверка_рандома() (
    ЗАДАТЬ р = ГенераторСлучайныхЧисел();
    ЦИКЛ ОТ 1 ДО 3 (
        НАПЕЧАТАТЬ р:получить_число();
    )
    ЦИКЛ ОТ 1 ДО 3 (
        НАПЕЧАТАТЬ р:получить_в_диапазоне(1, 100);
    )
    ЦИКЛ ОТ 1 ДО 3 (
        НАПЕЧАТАТЬ р:получить_целое_в_диапазоне(1, 100);
    )
    ЦИКЛ ОТ 1 ДО 3 (
        НАПЕЧАТАТЬ р:бросить_монетку();
    )
)


ВЫПОЛНИТЬ (
    проверка_очереди();
    проверка_списков();
    проверка_рандома();
)
