ВКЛЮЧИТЬ стандартная_библиотека.структуры.очереди
ВКЛЮЧИТЬ стандартная_библиотека.структуры.сложные_структуры


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ проверка_очереди() (
    ЗАДАТЬ тест = очередь(2);

    НАПЕЧАТАТЬ тест:добавить(1);
    НАПЕЧАТАТЬ тест:добавить(2);
    НАПЕЧАТАТЬ тест:добавить(2);
    НАПЕЧАТАТЬ тест:добавить(2);

    НАПЕЧАТАТЬ тест:взять();
    НАПЕЧАТАТЬ тест:взять();
    НАПЕЧАТАТЬ тест:взять();
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ проверка_списков() (
    ЗАДАТЬ с = список(массив(1, 2, 3));
    !ЗАДАТЬ с = список();

    с:добавить(1);

    НАПЕЧАТАТЬ с:в_массив();
    НАПЕЧАТАТЬ с:сортировать();

    с:удалить(-1);
    НАПЕЧАТАТЬ с:в_массив();


)

ВЫПОЛНИТЬ (
    проверка_очереди();
    проверка_списков();
)
