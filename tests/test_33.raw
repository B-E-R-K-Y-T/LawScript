ВКЛЮЧИТЬ стандартная_библиотека.*
ВКЛЮЧИТЬ util

ОПРЕДЕЛИТЬ КЛАСС Тест (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (_) () (
    )
    ОПРЕДЕЛИТЬ МЕТОД (_) метод (а, б) (
        НАПЕЧАТАТЬ а;
        НАПЕЧАТАТЬ б;
    )
)


ОПРЕДЕЛИТЬ ОБЪЕКТ тест_объект (
    ИМЯ имечкко,
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ функция(аргумент, sd) (
    НАПЕЧАТАТЬ аргумент;
    НАПЕЧАТАТЬ sd;
   ВЕРНУТЬ 115;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ главная() (
    НАПЕЧАТАТЬ тест_колбека(функция);
    ЗАДАТЬ т=  Тест();
    ЗАДАТЬ тест = т:метод;

    НАПЕЧАТАТЬ "-" * 10;
    НАПЕЧАТАТЬ тест_колбека(тест);

    !НАПЕЧАТАТЬ тест_колбека(Тест);
    ЗАДАТЬ т1 = тест_объект:тест;

    т1 = 213;

    НАПЕЧАТАТЬ т1;
    НАПЕЧАТАТЬ тест_объект:тест;

    тест_объект:__имя_объекта__ = 123;
    НАПЕЧАТАТЬ тест_объект:__имя_объекта__;
    НАПЕЧАТАТЬ МЕГА_ТЕСТ(ПУСТОТА);
)

ВЫПОЛНИТЬ (
    главная();
)
