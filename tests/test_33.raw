ВКЛЮЧИТЬ стандартная_библиотека.*
ВКЛЮЧИТЬ util

ОПРЕДЕЛИТЬ КЛАСС Тест (
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (_) () (
    )
    ОПРЕДЕЛИТЬ МЕТОД (_) _ (а, б) (
        НАПЕЧАТАТЬ а;
        НАПЕЧАТАТЬ б;
    )
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ функция(аргумент, sd) (
    НАПЕЧАТАТЬ аргумент;
    НАПЕЧАТАТЬ sd;
   ВЕРНУТЬ 115;
)

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ главная() (
    НАПЕЧАТАТЬ тест_колбека(функция);
    ЗАДАТЬ т=  Тест();
    ЗАДАТЬ тест = т:_;

    НАПЕЧАТАТЬ "-" * 10;
    НАПЕЧАТАТЬ тест_колбека(тест);

    !НАПЕЧАТАТЬ тест_колбека(Тест);
)

ВЫПОЛНИТЬ (
    главная();
)
