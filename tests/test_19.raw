ОПРЕДЕЛИТЬ КЛАСС дед НАСЛЕДОВАТЬ ОТ супер_дед(
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка) (число) (
        НАПЕЧАТАТЬ "ДЕД";
        ссылка:число = число;
        ссылка:число_деда = 123;
        ссылка:_очередь = Очередь(100);
        ДОКУМЕНТАЦИЯ (
            базовый класс
        )
    )
    ОПРЕДЕЛИТЬ МЕТОД (_) м() (
        ВЕРНУТЬ 3000000;
    )
    ОПРЕДЕЛИТЬ МЕТОД (_) добавить(проц) (
        ссылка:_очередь:положить(проц);
    )
    ОПРЕДЕЛИТЬ МЕТОД (_) взять() (
        ВЕРНУТЬ ссылка:_очередь:взять();
    )
)


ОПРЕДЕЛИТЬ КЛАСС родитель НАСЛЕДОВАТЬ ОТ дед(
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка) (число) (
        НАПЕЧАТАТЬ "РОДИТЕЛЬ";
        ссылка:__родитель__:__конструктор__(число);
        ссылка:число_деда = 1;
    )
    ОПРЕДЕЛИТЬ МЕТОД (_) м() (
        ВЕРНУТЬ 1000000;
    )

)

ОПРЕДЕЛИТЬ КЛАСС ребенок НАСЛЕДОВАТЬ ОТ родитель(
    ОПРЕДЕЛИТЬ КОНСТРУКТОР (ссылка) (число=100) (
        НАПЕЧАТАТЬ "РЕБЕНОК";
        ссылка:__родитель__:__конструктор__(число);
        НАПЕЧАТАТЬ ссылка:__родитель__:м();
        НАПЕЧАТАТЬ ссылка:__родитель__:__родитель__:м();
        НАПЕЧАТАТЬ ссылка:число;
        НАПЕЧАТАТЬ ссылка:м();
        НАПЕЧАТАТЬ ссылка:__родитель__:__родитель__:число_деда;
        НАПЕЧАТАТЬ ссылка:добавить(вызов);
    )
    ОПРЕДЕЛИТЬ МЕТОД (_) м() (
        ВЕРНУТЬ 2000000;
    )
)


