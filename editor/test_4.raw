ВКЛЮЧИТЬ стандартная_библиотека.*
!ВКЛЮЧИТЬ dec
!ВКЛЮЧИТЬ imp

ОПРЕДЕЛИТЬ ПРОЦЕДУРУ печать(а) (
	НАПЕЧАТАТЬ а;
!	1/0;
)


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ вход() (
	ЗАДАТЬ о = Очередь(1);
	
	НАПЕЧАТАТЬ массив(о:пуста(), о:пуста(), о:пуста());
	
	о:положить(печать);

	НАПЕЧАТАТЬ массив(о:пуста(), о:пуста(), о:пуста());

	ЗАДАТЬ ф = о:взять();
	ф("ПРИВЕТ");
	НАПЕЧАТАТЬ массив(о:пуста(), о:пуста(), о:пуста());

)

ВЫПОЛНИТЬ (
	вход();
)







