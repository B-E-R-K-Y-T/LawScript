ВКЛЮЧИТЬ стандартная_библиотека.структуры.примитивные_структуры


ОПРЕДЕЛИТЬ ПРОЦЕДУРУ отобразить_на_массив(отображаемая_функция, коллекция)(
    ЗАДАТЬ новая_коллекция = массив();
    ЗАДАТЬ длина = длина_массива(коллекция);

    ЦИКЛ счет ОТ 0 ДО длина - 1 (
        добавить_в_массив(новая_коллекция, отображаемая_функция(достать_из_массива(коллекция, счет)));
    )

    ВЕРНУТЬ новая_коллекция;
)
