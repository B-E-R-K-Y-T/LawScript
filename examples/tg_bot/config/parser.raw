ОПРЕДЕЛИТЬ ПРОЦЕДУРУ парсер() (
    ЗАДАТЬ строки = прочитать_файл(".env");
    ЗАДАТЬ таблица_строк_файла = таблица();
    ЗАДАТЬ кол_во_строк = длина_массива(строки);
    ЗАДАТЬ пара;

    ЦИКЛ сдвиг ОТ 0 ДО кол_во_строк - 1 (
        пара = разделить_строку(достать_из_массива(строки, сдвиг), "=");

        добавить_в_таблицу(таблица_строк_файла, достать_из_массива(пара, 0), достать_из_массива(пара, 1));
    )

    ВЕРНУТЬ таблица_строк_файла;
)
