ОПРЕДЕЛИТЬ ПРОЦЕДУРУ Премия(премия)(
    ЗАДАТЬ ЗП_С = 100000;

    ЗАДАТЬ результат = (1 - 2 / 100 * 13) + 3;


    НАПЕЧАТАТЬ -(-(-(-(-(-(-(1)))))));
    НАПЕЧАТАТЬ 2^2+2*2-2+2^2^2;
    НАПЕЧАТАТЬ (2 ^ 2 + 2 * 2 - 2 + 2 ^ 2 ^ 2);
    НАПЕЧАТАТЬ 2 ^ 2 + 2 * 2;

    НАПЕЧАТАТЬ "Премия, которую хочет сотрудник: ";
    НАПЕЧАТАТЬ премия;

    ЗАДАТЬ ЗП_С = 100000;
    ЗАДАТЬ результат = (ЗП_С - (ЗП_С / 100) * 13) + премия;

    ЦИКЛ ОТ 1 ДО 100000 (
        НАПЕЧАТАТЬ (ЗП_С - (ЗП_С / 100) * 13) + премия;
    )

    ЕСЛИ результат БОЛЬШЕ 200000 ТО (
        НАПЕЧАТАТЬ "Премия, которую можем выдать: ";
        НАПЕЧАТАТЬ результат;
        ВЕРНУТЬ 200000;
    )

    НАПЕЧАТАТЬ "Премия, которую можем выдать: ";
    НАПЕЧАТАТЬ результат;

    ВЕРНУТЬ результат;
)

